pipeline {

    stages {
	      tools{
              jdk 'JAVA_HOME'
              maven 'M2_HOME'
			  }
        stage('git clone') {
           
           agent{label 'linuxslave'}
           steps{
               git 'https://github.com/Shradha1907/mvn-jenkins.git'
                sh 'mvn compile'
           }

           }

         tools{
              jdk 'JAVA_HOME1'
              maven 'M2_HOME1'
               }
   stage('git clone') {
           
           agent{
              label 'windowsslave'
           }
           steps{
               git 'https://github.com/Shradha1907/mvn-jenkins.git'
                sh 'mvn compile'
           }

           }     
           
    }
	}
